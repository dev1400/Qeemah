
<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="sapui5tutorial1.Overview" xmlns:layout="sap.ui.commons.layout"
	xmlns:f="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Special Track">
		<content>
			<VBox>
				<Input type="Text" placeholder="Enter User name" />
				<Input type="Text" placeholder="Enter Password" />
				<HBox justifyContent="Center">
					<layout:MatrixLayout id="idMatrixLayout"
						layoutFixed="false">
						<layout:MatrixLayoutRow>
							<layout:MatrixLayoutCell>
								<Button type="Accept" text="Ok" press="goToNextPage">
								</Button>
							</layout:MatrixLayoutCell>
							<layout:MatrixLayoutCell>
								<Button type="Reject" text="Cancel" press="onPress">
								</Button>
							</layout:MatrixLayoutCell>

						</layout:MatrixLayoutRow>
					</layout:MatrixLayout>
				</HBox>
			</VBox>
			<f:SimpleForm id="SimpleFormChange354" minWidth="1024"
				maxContainerCols="2" editable="true" layout="ResponsiveGridLayout"
				title="Login" labelSpanL="3" labelSpanM="3" emptySpanL="4"
				emptySpanM="4" columnsL="1" columnsM="1" class="editableForm">

				<f:content>
					<Input type="Text" placeholder="Enter User name" />
					<Input type="Text" placeholder="Enter Password" />
					<layout:MatrixLayout id="idMatrixLayout2"
						layoutFixed="false">
						<layout:MatrixLayoutRow>
							<layout:MatrixLayoutCell>
								<Button type="Accept" text="Ok" press="goToNextPage">
								</Button>
							</layout:MatrixLayoutCell>
							<layout:MatrixLayoutCell>
								<Button type="Reject" text="Cancel" press="onPress">
								</Button>
							</layout:MatrixLayoutCell>

						</layout:MatrixLayoutRow>
					</layout:MatrixLayout>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</core:View>