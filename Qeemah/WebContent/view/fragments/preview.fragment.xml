<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:sap.ui.unified="sap.ui.unified" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">


	<VBox class="HeadingBGVBOX">


		<sap.ui.commons:TabStrip class="IconTabBarStyling" width="100%">
			<sap.ui.commons:tabs>
				<sap.ui.commons:Tab text="{i18n>Preview}">
					<sap.ui.commons:content>

						<VBox justifyContent="Center" alignItems="Center">

							<sap.ui.commons.layout:MatrixLayout layoutFixed="true" width="100%">
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell
												hAlign="Center">
												<sap.ui.commons.layout:content>
													<VBox id="idPreviewOranizationInfoVBox" visible="true">

														<sap.ui.core:Fragment
															fragmentName="com.sagia.view.fragments.preview_orginfo_dialog"
															type="XML" width="100%"/>
													</VBox>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>



									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell
												hAlign="Center">
												<sap.ui.commons.layout:content>
													<VBox id="idPreviewContactInfoVBox" visible="true">
														<html:br />
														<sap.ui.core:Fragment
															fragmentName="com.sagia.view.fragments.preview_contactinfo_dialog"
															type="XML" width="100%"/>
													</VBox>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>


									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell
												hAlign="Center">
												<sap.ui.commons.layout:content>
													<VBox id="idPreviewBAAInfoVBox" visible="true">
														<html:br />
														<sap.ui.core:Fragment
															fragmentName="com.sagia.view.fragments.preview_bizactivityq_dialog"
															type="XML" width="100%"/>
													</VBox>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>



									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell
												hAlign="Center">
												<sap.ui.commons.layout:content>
													<VBox id="idPreviewLicenseInfoVBox" justifyContent="Center"
														alignItems="Center" visible="true" width="100%">
														<sap.ui.core:Fragment
															fragmentName="com.sagia.view.fragments.preview_licenseinfo_dialog"
															type="XML" width="100%" />
													</VBox>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>


									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell
												hAlign="Center">
												<sap.ui.commons.layout:content>
													<VBox id="idPreviewShareHolderDetailsInfoVBox"
														visible="true">
														<html:br />
														<sap.ui.core:Fragment
															fragmentName="com.sagia.view.fragments.preview_shareholderdetails_dialog"
															type="XML" width="100%"/>
													</VBox>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
							</sap.ui.commons.layout:MatrixLayout>



								<sap.ui.commons.layout:MatrixLayout>

									<sap.ui.commons.layout:rows>
										<sap.ui.commons.layout:MatrixLayoutRow>
											<sap.ui.commons.layout:cells>
												<sap.ui.commons.layout:MatrixLayoutCell
													hAlign="End">
													<sap.ui.commons.layout:content>
														<Button style="Accept" text="{i18n>Previous}"
															id="idPreviewInfoPreviousButton" press="handlePreviewPreviousButtonPress"
															icon="sap-icon://navigation-left-arrow">
														</Button>
													</sap.ui.commons.layout:content>
												</sap.ui.commons.layout:MatrixLayoutCell>

												<sap.ui.commons.layout:MatrixLayoutCell
													hAlign="Begin">
													<sap.ui.commons.layout:content>
														<Button style="Accept" text="{i18n>Next}"
															id="idPreviewInfoNextButton" press="handlePreviewNextButtonPress"
															icon="sap-icon://navigation-right-arrow">
														</Button>
													</sap.ui.commons.layout:content>
												</sap.ui.commons.layout:MatrixLayoutCell>
											</sap.ui.commons.layout:cells>
										</sap.ui.commons.layout:MatrixLayoutRow>
									</sap.ui.commons.layout:rows>


								</sap.ui.commons.layout:MatrixLayout>




						</VBox>


					</sap.ui.commons:content>
				</sap.ui.commons:Tab>
			</sap.ui.commons:tabs>
		</sap.ui.commons:TabStrip>
	</VBox>

</sap.ui.core:FragmentDefinition>