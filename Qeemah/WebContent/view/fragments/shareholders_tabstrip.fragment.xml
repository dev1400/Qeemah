										</sap.ui.commons.layout:content>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<sap.ui.commons.layout:content>
											<sap.ui.commons:DatePicker id="DatePicker-Birth"
												value="Aug 8, 1980" editable="false" yyyymmdd="19800808"
