<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:sap.ui.commons="sap.ui.commons" xmlns:sap.ui.core="sap.ui.core"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.core.mvc="sap.ui.core.mvc"
	xmlns:sap.ui.unified="sap.ui.unified" xmlns:html="http://www.w3.org/1999/xhtml">
	
	<VBox id="idShareHoldersContentVBox" class="HeadingBGVBOX">
		<HBox justifyContent="Center">
		<sap.ui.commons:TabStrip class="IconTabBarStyling"
				>
				<sap.ui.commons:tabs>
					<sap.ui.commons:Tab  text="{i18n>Shareholder}">
						<sap.ui.commons:content>
						<VBox justifyContent="Center" alignItems="Center">

							<sap.ui.core:Fragment
								fragmentName="com.sagia.view.fragments.existingshareholder"
								type="XML" />
								</VBox>
		
		
		
			<VBox>
		
		
				<sap.ui.commons.layout:MatrixLayout>
		
					<sap.ui.commons.layout:rows>
						<sap.ui.commons.layout:MatrixLayoutRow>
							<sap.ui.commons.layout:cells>
								<sap.ui.commons.layout:MatrixLayoutCell
									hAlign="End">
									<sap.ui.commons.layout:content>
										<!-- <Button style="Accept" text="{i18n>BackToBAQ}" press="" icon="sap-icon://navigation-up-arrow"> 
											</Button> -->
										<Button style="Accept" text="{i18n>Previous}" id="idSHInfoPreviousButton"
											press="handleSHPreviousButtonPress" icon="sap-icon://navigation-left-arrow">
										</Button>
									</sap.ui.commons.layout:content>
								</sap.ui.commons.layout:MatrixLayoutCell>
		
								<sap.ui.commons.layout:MatrixLayoutCell
									hAlign="Begin">
									<sap.ui.commons.layout:content>
										<Button style="Accept" text="{i18n>Next}" id="idSHInfoNextButton"
											press="handleSHNextButtonPress" icon="sap-icon://navigation-right-arrow">
										</Button>
									</sap.ui.commons.layout:content>
								</sap.ui.commons.layout:MatrixLayoutCell>
							</sap.ui.commons.layout:cells>
						</sap.ui.commons.layout:MatrixLayoutRow>
					</sap.ui.commons.layout:rows>
		
		
		
				</sap.ui.commons.layout:MatrixLayout>
		
		
			</VBox>
							
							
							
						</sap.ui.commons:content>
					</sap.ui.commons:Tab>
					
				</sap.ui.commons:tabs>
			</sap.ui.commons:TabStrip>
			<!-- <IconTabBar class="iconTabBarPaddingTop IconTabBarStyling"
				expanded="true" expandable="false">
				<items>
					<IconTabFilter text="{i18n>ShareHolder}">

						<VBox justifyContent="Center" alignItems="Center">

							<sap.ui.core:Fragment
								fragmentName="com.sagia.view.fragments.existingshareholder"
								type="XML" />
						</VBox>
						
					</IconTabFilter>
					
				</items>
			</IconTabBar> -->
		</HBox>
	</VBox>
	
	<!-- </ScrollContainer> -->

</core:FragmentDefinition>