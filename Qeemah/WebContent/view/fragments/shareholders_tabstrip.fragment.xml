<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:sap.ui.commons="sap.ui.commons" xmlns:sap.ui.core="sap.ui.core"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.core.mvc="sap.ui.core.mvc">
	<sap.ui.commons:TabStrip id="TabStrip1" height="60em"
		width="80em">
		<sap.ui.commons:customData>
			<sap.ui.core:CustomData key="sap-ui-fastnavgroup"
				value="true" writeToDom="true"></sap.ui.core:CustomData>
		</sap.ui.commons:customData>
		<sap.ui.commons:tabs>
			<sap.ui.commons:Tab id="TabStrip1-0"
				text="{i18n>ExistingShareHolder}">
				<sap.ui.commons:content>

					<sap.ui.commons.layout:MatrixLayout>
					<sap.ui.commons.layout:rows>
						<sap.ui.commons.layout:MatrixLayoutRow>
							<sap.ui.commons.layout:cells>
								<sap.ui.commons.layout:MatrixLayoutCell colSpan="2">
									<core:HTML content="{i18n>ProductsforIndustrialLicenseType}">
									</core:HTML>
								</sap.ui.commons.layout:MatrixLayoutCell>
							</sap.ui.commons.layout:cells>
						</sap.ui.commons.layout:MatrixLayoutRow>
						<sap.ui.commons.layout:MatrixLayoutRow>
							<sap.ui.commons.layout:cells>
								<sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:content>
										<sap.ui.commons.layout:MatrixLayout layoutFixed="false">
											<sap.ui.commons.layout:MatrixLayoutRow>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<HBox justifyContent="End">
														<core:HTML content="{i18n>LicenseActivityHTML}">
														</core:HTML>
													</HBox>
												</sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<ComboBox width="25em">
														<core:Item />
													</ComboBox>
												</sap.ui.commons.layout:MatrixLayoutCell>
											</sap.ui.commons.layout:MatrixLayoutRow>
											<sap.ui.commons.layout:MatrixLayoutRow>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<HBox justifyContent="End">
														<core:HTML content="{i18n>QuantityHTML}">
														</core:HTML>
													</HBox>
												</sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<ComboBox width="25em">
														<core:Item />
													</ComboBox>
												</sap.ui.commons.layout:MatrixLayoutCell>
											</sap.ui.commons.layout:MatrixLayoutRow>
											<sap.ui.commons.layout:MatrixLayoutRow>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<HBox justifyContent="End">
														<core:HTML content="{i18n>UnitOfMeasurementHTML}">
														</core:HTML>
													</HBox>
												</sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<ComboBox width="25em">
														<core:Item />
													</ComboBox>
												</sap.ui.commons.layout:MatrixLayoutCell>
											</sap.ui.commons.layout:MatrixLayoutRow>
											<sap.ui.commons.layout:MatrixLayoutRow>
												<sap.ui.commons.layout:MatrixLayoutCell>
													<Button type="Accept" text="{i18n>Add}" />
												</sap.ui.commons.layout:MatrixLayoutCell>
											</sap.ui.commons.layout:MatrixLayoutRow>
										</sap.ui.commons.layout:MatrixLayout>
									</sap.ui.commons.layout:content>
								</sap.ui.commons.layout:MatrixLayoutCell>
							</sap.ui.commons.layout:cells>
						</sap.ui.commons.layout:MatrixLayoutRow>
						<sap.ui.commons.layout:MatrixLayoutRow>
							<sap.ui.commons.layout:cells>
								<sap.ui.commons.layout:MatrixLayoutCell colSpan="2">
									<sap.ui.commons.layout:content>
										<Table width="60em">
											<columns>
												<Column width="2em">
													<Text text="#" />
												</Column>
												<Column width="5em">
													<Text text="{i18n>ProductCode}" />
												</Column>
												<Column width="5em">
													<Text text="{i18n>ProductName}" />
												</Column>
												<Column width="5em">
													<Text text="{i18n>Quantity}" />
												</Column>
												<Column width="5em">
													<Text text="{i18n>UnitOfMeasurement}" />
												</Column>
												<Column width="5em">
													<Text text="{i18n>Delete}" />
												</Column>
											</columns>
											<items>
												<ColumnListItem>
													<cells>
														<Input type="Text" />
														<Input type="Text" />
														<Input type="Text" />
														<Input type="Text" />
														<Input type="Text" />
														<Input type="Text" />
													</cells>
												</ColumnListItem>
											</items>
										</Table>
									</sap.ui.commons.layout:content>
								</sap.ui.commons.layout:MatrixLayoutCell>
							</sap.ui.commons.layout:cells>
						</sap.ui.commons.layout:MatrixLayoutRow>
					</sap.ui.commons.layout:rows>
					</sap.ui.commons.layout:MatrixLayout>


				</sap.ui.commons:content>
				<sap.ui.commons:title>
					<sap.ui.core:Title id="TabStrip1-0-title"
						text="{i18n>ExistingShareHolder}"></sap.ui.core:Title>
				</sap.ui.commons:title>
			</sap.ui.commons:Tab>
			<sap.ui.commons:Tab id="tab2" text="{i18n>NewShareHolder}"
				tooltip="{i18n>NewShareHolder}">
				<sap.ui.commons:content>
					
				</sap.ui.commons:content>
				<sap.ui.commons:title>
					<sap.ui.core:Title id="Title2" text="{i18n>NewShareHolder}"></sap.ui.core:Title>
				</sap.ui.commons:title>
			</sap.ui.commons:Tab>

		</sap.ui.commons:tabs>
	</sap.ui.commons:TabStrip>
</core:FragmentDefinition>