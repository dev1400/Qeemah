<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:sap.ui.commons="sap.ui.commons" xmlns:sap.ui.core="sap.ui.core"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.core.mvc="sap.ui.core.mvc" xmlns:sap.ui.unified="sap.ui.unified">
	<ScrollContainer
    height="40rem"
    width="70rem"
    vertical="true" >
	
	<VBox id="idShareHoldersContentVBox" class="HeadingBGVBOX"
		>
		<HBox justifyContent="Center">
		<IconTabBar
    class="iconTabBarPaddingTop IconTabBarHeadingBGCOlor"
    expanded="true" expandable="false" >
    <items>
      <IconTabFilter
        text="{i18n>ExistingShareHolder}">
                <VBox justifyContent="Center" alignItems="Center">
        
         <sap.ui.core:Fragment
						fragmentName="com.sagia.view.fragments.existingshareholder"
						type="XML" />
						</VBox>
      </IconTabFilter>
      <IconTabFilter
        text="{i18n>NewShareHolder}">
        <VBox justifyContent="Center" alignItems="Center">
        <sap.ui.core:Fragment
						fragmentName="com.sagia.view.fragments.newshareholder"
						type="XML" />
						</VBox>
        
      </IconTabFilter>
    </items>
  </IconTabBar></HBox>
	</VBox>
	</ScrollContainer>
</core:FragmentDefinition>