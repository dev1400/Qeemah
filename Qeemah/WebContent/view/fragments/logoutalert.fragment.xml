<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core" xmlns:sap.ui.commons="sap.ui.commons"
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
  <Dialog
    icon="sap-icon://alert"
    title="{i18n>Information}"
    class="sapUiPopupWithPadding" >
    <content>
      <VBox id="idLogoutAlert" justifyContent="Center" alignItems="Center">
      
        <sap.ui.commons:TextView id="idLogoutAlertFragmentTextView" text="{i18n>WouldYouLikeToSave}"></sap.ui.commons:TextView>
        
	  </VBox>	 
    </content>
    <beginButton>
    <Button text="{i18n>Yes}" app:dialogType="Std" press="handleLogoutAlertYesDialogButtonPress" />
    </beginButton>
    <endButton>      
      <Button text="{i18n>No}" app:dialogType="Std" press="handleLogoutAlertNoDialogButtonPress" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>