											<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
										
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<MultiComboBox selectionChange="handleLILIClassMultiSelectionComboBoxChange"  id="idLILIClassMultiComboBox"
										selectionFinish="handleSelectionFinish"    items="{/LILIClassCollection}" width="25em">
                                        <sap.ui.core:ListItem key="{IsicClass}" text="{IsicDescription}" id="idLILIClassMultiComboBoxListitem"/> <!-- key="{IsicClass}"  -->
