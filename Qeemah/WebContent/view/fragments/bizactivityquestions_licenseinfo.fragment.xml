<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.unified="sap.ui.unified"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<!-- Child license activity Q start -->
	<sap.ui.commons.layout:MatrixLayout
		layoutFixed="false">
		<sap.ui.commons.layout:rows>
			<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons.layout:content>
							<sap.ui.commons.layout:MatrixLayout
								layoutFixed="false">
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>Section}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>Division}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>Group}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>Class}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>LicenseActivity}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>ActivityDescription}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<sap.ui.commons.layout:content>
											<TextArea width="25em" />
										</sap.ui.commons.layout:content>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								
								
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>LicenseActivity}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>Quantity}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<HBox justifyContent="End">
											<Label text="{i18n>UnitOfMeasurement}"></Label>
										</HBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<ComboBox width="25em">
											<sap.ui.core:Item />
										</ComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<Button type="Accept" text="{i18n>Add}" />
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
							
							</sap.ui.commons.layout:MatrixLayout>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
		</sap.ui.commons.layout:rows>
	</sap.ui.commons.layout:MatrixLayout>
	<sap.ui.commons.layout:MatrixLayout
		layoutFixed="false">
		<sap.ui.commons.layout:rows>
			
			
			<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell
						colSpan="2">
						<sap.ui.commons.layout:content>
							<Table width="60em">
								<columns>
									<Column width="2em">
										<Text text="#" />
									</Column>
									<Column width="5em">
										<Label text="{i18n>ProductCode}"></Label>
									</Column>
									<Column width="5em">
										
										<Label text="{i18n>ProductName}"></Label>
									</Column>
									<Column width="5em">
										<Label text="{i18n>Quantity}"></Label>
									</Column>
									<Column width="5em">
										<Label text="{i18n>UnitOfMeasurement}"></Label>
									</Column>
									<Column width="5em">
										<Label text="{i18n>Delete}"></Label>
									</Column>
								</columns>
								<items>
									<ColumnListItem>
										<cells>
											<Input type="Text" />
											<Input type="Text" />
											<Input type="Text" />
											<Input type="Text" />
											<Input type="Text" />
											<Input type="Text" />
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
		</sap.ui.commons.layout:rows>
	</sap.ui.commons.layout:MatrixLayout>
	<!-- Business Activity Questions End -->
	<!-- Child license activity Q end -->
	<!-- <VBox alignItems="Center">
	<Button style="Accept" text="{i18n>Next}" press="" >
							</Button>
	</VBox> -->
</sap.ui.core:FragmentDefinition>