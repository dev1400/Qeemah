											<sap.ui.commons:TextView text="{i18n>BusinessType}"></sap.ui.commons:TextView>
											<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
										
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell >
										<ComboBox width="25em" items="{/LILIBusinessTypeCollection}" id="idLILIBusinessTypeComboBox" selectionChange="handleLILIBusinessTypeComboBoxSelectionChange">
										     <sap.ui.core:ListItem key="{Flag}" text="{IsicDescription}" id="idLILIBusinessTypeComboBoxitem"/>
										</ComboBox>
									
									</sap.ui.commons.layout:MatrixLayoutCell>
