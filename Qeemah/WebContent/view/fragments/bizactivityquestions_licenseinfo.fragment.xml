									<sap.ui.commons.layout:MatrixLayoutCell>
										
											<sap.ui.commons:TextView text="{i18n>LicenseActivity}"></sap.ui.commons:TextView>
											<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<MultiComboBox selectionChange="handleLILILicenseActivityMultiSelectionComboBoxChange"  id="idLILILicenseActivityMultiComboBox"
										selectionFinish="handleSelectionFinish"    items="{/LILILicenseActivityCollection}" width="25em">
                                        <sap.ui.core:ListItem key="{Act}" text="{IsicDescription}" id="idLILILicenseActivityMultiComboBoxListitem"/> <!-- key="{IsicClass}"  -->
                                     </MultiComboBox>
									</sap.ui.commons.layout:MatrixLayoutCell>
									</sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons:TextView text="{i18n>ActivityDescription}"></sap.ui.commons:TextView>
									</sap.ui.commons.layout:MatrixLayoutCell>
									<sap.ui.commons.layout:MatrixLayoutCell>
										<sap.ui.commons.layout:content>
											<TextArea width="25em"  id="idLILIActivityDescriptionTextArea"/>
										</sap.ui.commons.layout:content>
									</sap.ui.commons.layout:MatrixLayoutCell>
								</sap.ui.commons.layout:MatrixLayoutRow>
		
	</sap.ui.commons.layout:MatrixLayout>
