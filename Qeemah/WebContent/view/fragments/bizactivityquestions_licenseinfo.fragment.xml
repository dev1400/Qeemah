			<VBox id="idProductsTableVBox" visible="false" alignItems="Center">
		
		<sap.ui.commons.layout:MatrixLayout
		layoutFixed="false">
		
		<sap.ui.commons.layout:MatrixLayoutRow>
			<sap.ui.commons.layout:cells>
				<sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons:TextView text="{i18n>Product}"></sap.ui.commons:TextView>
				</sap.ui.commons.layout:MatrixLayoutCell>
				<sap.ui.commons.layout:MatrixLayoutCell>					
					<ComboBox width="25em" items="{/IndustrialProductsCollection}" id="idLILIIndustrialProductComboBox" selectionChange="handleLILIIndustrialProductsComboBoxSelectionChange">
						<sap.ui.core:ListItem key="{Pcode}" text="{Desc}" id="idLILIIndustrialProductComboBoxitem"/>
					</ComboBox>
				</sap.ui.commons.layout:MatrixLayoutCell>
			</sap.ui.commons.layout:cells>
		</sap.ui.commons.layout:MatrixLayoutRow>
		<sap.ui.commons.layout:MatrixLayoutRow>
			<sap.ui.commons.layout:cells>
				<sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons:TextView text="{i18n>Quantity}"></sap.ui.commons:TextView>
