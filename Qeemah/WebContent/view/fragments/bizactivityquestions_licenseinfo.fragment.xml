					<ComboBox width="25em" items="{/IndustrialProductsCollectionUOM}" id="idLILIIndustrialProductUOMComboBox" selectionChange="handleLILIIndustrialProductsUOMComboBoxSelectionChange">
						<sap.ui.core:ListItem key="{Msehi}" text="{Msehl}" id="idLILIIndustrialProductUOMComboBoxitem"/>
					</ComboBox>
				</sap.ui.commons.layout:MatrixLayoutCell>
			</sap.ui.commons.layout:cells>
