<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:sap.ui.unified="sap.ui.unified" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<sap.ui.commons.layout:MatrixLayout id="idNewShareHolderActivityQuestionsMLAyout"
		layoutFixed="false" width="78em">
		<sap.ui.commons.layout:rows>
		
		<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="Begin" colSpan="2">
						<sap.ui.commons.layout:content>
							<sap.ui.commons:TextView text="{i18n>ActivityQuestions}" design="H1"></sap.ui.commons:TextView>
							<sap.ui.commons:HorizontalDivider></sap.ui.commons:HorizontalDivider>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
					
					
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
			<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="Begin">
						<sap.ui.commons.layout:content>
						<sap.ui.commons:TextView text="{i18n>AgeOfParentCompany}"></sap.ui.commons:TextView>
						<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
						<sap.ui.commons.layout:content>
						<ComboBox id="idNSHActivityQ1ComboBox" width="25em">
								<sap.ui.core:ListItem Key="1" text="1-10" />
								<sap.ui.core:ListItem key="2" text="11-20" />
								<sap.ui.core:ListItem Key="3" text="21-30" />
								<sap.ui.core:ListItem key="4" text="31-40" />
								<sap.ui.core:ListItem key="5" text="41-50" />
								<sap.ui.core:ListItem key="6" text="{i18n>Above}" />
									</ComboBox>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
			<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="Begin">
						<sap.ui.commons.layout:content>
						<sap.ui.commons:TextView text="{i18n>HowManyBranches}"></sap.ui.commons:TextView>
						<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
						<sap.ui.commons.layout:content>
						<ComboBox id="idNSHActivityQ2ComboBox" width="25em">
								<sap.ui.core:ListItem Key="1" text="1-10" />
								<sap.ui.core:ListItem key="2" text="11-50" />
								<sap.ui.core:ListItem Key="3" text="51-100" />
								<sap.ui.core:ListItem key="4" text="101-150" />
								<sap.ui.core:ListItem key="5" text="151-200" />
								<sap.ui.core:ListItem key="6" text="{i18n>Above}" />
									</ComboBox>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
			<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="Begin">
						<sap.ui.commons.layout:content>
						<sap.ui.commons:TextView text="{i18n>HowManyEmployees}"></sap.ui.commons:TextView>
						<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
						<sap.ui.commons.layout:content>
						<ComboBox id="idNSHActivityQ3ComboBox" width="25em">
								<sap.ui.core:ListItem Key="1" text="1-10" />
								<sap.ui.core:ListItem key="2" text="11-100" />
								<sap.ui.core:ListItem Key="3" text="101-500" />
								<sap.ui.core:ListItem key="4" text="501-1000" />
								<sap.ui.core:ListItem key="5" text="1001-2000" />
								<sap.ui.core:ListItem key="6" text="{i18n>Above}" />
									</ComboBox>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
			<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell colSpan="2" hAlign="End">
						<sap.ui.commons.layout:content>
						<sap.ui.unified:FileUploader id="idActivityQuestionsFU" width="18.7rem" 
						placeholder="{i18n>SupportingDocuments}" icon="common/mime/attachment.png" iconOnly="true"/>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
			
