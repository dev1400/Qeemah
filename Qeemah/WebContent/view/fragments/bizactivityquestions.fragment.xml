<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.unified="sap.ui.unified"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<!-- Business Activity Quastions Start -->




	<sap.ui.commons.layout:MatrixLayout
		id="idLI_BAQ_1_to_6MAtrixLayoutz" layoutFixed="true" widths="8rem">
		<sap.ui.commons.layout:rows>
		</sap.ui.commons.layout:rows>
	</sap.ui.commons.layout:MatrixLayout>


	<sap.ui.commons.layout:MatrixLayout
		id="idLI_BAQ_7_to_10MatrixLayout" visible="false">
	</sap.ui.commons.layout:MatrixLayout>
	<VBox alignItems="Center" justifyContent="Center">

		<sap.ui.commons.layout:MatrixLayout>

			<sap.ui.commons.layout:rows>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:cells>
						<sap.ui.commons.layout:MatrixLayoutCell
							hAlign="Center">
							<sap.ui.commons.layout:content>

							</sap.ui.commons.layout:content>
						</sap.ui.commons.layout:MatrixLayoutCell>
					</sap.ui.commons.layout:cells>
				</sap.ui.commons.layout:MatrixLayoutRow>
			</sap.ui.commons.layout:rows>
			
			<sap.ui.commons.layout:rows>
		<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="Begin" colSpan="4">
						<sap.ui.commons.layout:content>
							<BusyIndicator size="1.8em" id="idBAQbusyIndicator" text="{i18n>Loading}"/>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
					
					
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
		</sap.ui.commons.layout:rows>
		
		</sap.ui.commons.layout:MatrixLayout>

		<sap.ui.commons.layout:MatrixLayout>
			<sap.ui.commons.layout:rows>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:cells>
						<sap.ui.commons.layout:MatrixLayoutCell
							hAlign="End">
							<sap.ui.commons.layout:content>
								<!-- <Button style="Accept" text="{i18n>BackToBAQ}" press="" icon="sap-icon://navigation-up-arrow"> 
									</Button> -->
								<Button style="Accept" text="{i18n>Previous}"
									id="idLicenseInfoBAQPreviousButton" press="handleLIBAQPreviousButtonPress"
									icon="sap-icon://navigation-left-arrow">
								</Button>
							</sap.ui.commons.layout:content>
						</sap.ui.commons.layout:MatrixLayoutCell>

						<sap.ui.commons.layout:MatrixLayoutCell
							hAlign="Begin">
							<sap.ui.commons.layout:content>
								<Button style="Accept" text="{i18n>Next}" id="idLIBAQInfoNextButton"
									press="handleBAQNextButtonPress" icon="sap-icon://navigation-right-arrow"
									iconFirst="false"></Button>
							</sap.ui.commons.layout:content>
						</sap.ui.commons.layout:MatrixLayoutCell>
					</sap.ui.commons.layout:cells>
				</sap.ui.commons.layout:MatrixLayoutRow>
			</sap.ui.commons.layout:rows>
		</sap.ui.commons.layout:MatrixLayout>
	</VBox>

</sap.ui.core:FragmentDefinition>