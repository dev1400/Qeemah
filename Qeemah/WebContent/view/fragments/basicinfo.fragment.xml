<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:sap.ui.unified="sap.ui.unified" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">


	<VBox class="HeadingBGVBOX">

		<HBox id="idBasicInfoBoxContent" justifyContent="Center" width="100%">
			<IconTabBar class="iconTabBarPaddingTop IconTabBarStyling"
				expanded="true" expandable="false" select="handleBasicInfoIconTabBarSelect">
				<items>
					<IconTabFilter text="{i18n>Organization}" id="idIconTabFilterOrg"
						key="BasicInfoIconTabKey">
						 <sap.ui.core:Fragment fragmentName="com.sagia.view.fragments.bi_organization"
		type="XML" />

					
					</IconTabFilter>
					<IconTabFilter text="{i18n>ContactInformation}">
 <sap.ui.core:Fragment fragmentName="com.sagia.view.fragments.bi_contactinformation"
		type="XML" />
					
					</IconTabFilter>
				</items>
			</IconTabBar>









		</HBox>
	</VBox>

	
</sap.ui.core:FragmentDefinition>