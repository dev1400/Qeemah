<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.unified="sap.ui.unified"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
 <VBox width="74.44rem" class="HeadingBGVBOX">
	    <sap.ui.core:HTML content="{i18n>LicenseInformationHTML}"></sap.ui.core:HTML>
	<!-- License info start -->
	<sap.ui.commons:TabStrip height="64em"
		width="65rem">
		<sap.ui.commons:customData>
			<sap.ui.core:CustomData key="sap-ui-fastnavgroup"
				value="true" writeToDom="true"></sap.ui.core:CustomData>
		</sap.ui.commons:customData>
		<sap.ui.commons:tabs>
			<sap.ui.commons:Tab text="{i18n>BusinessActivityQuestions}">
				<sap.ui.commons:content>
				<sap.ui.core:Fragment fragmentName="com.sagia.view.fragments.bizactivityquestions"
		type="XML" />
				</sap.ui.commons:content>
			</sap.ui.commons:Tab>
			<sap.ui.commons:Tab text="{i18n>LicenseInfo}">
				<sap.ui.commons:content>
				<sap.ui.core:Fragment
		fragmentName="com.sagia.view.fragments.bizactivityquestions_licenseinfo"
		type="XML" />
				</sap.ui.commons:content>
			</sap.ui.commons:Tab>
		</sap.ui.commons:tabs>
	</sap.ui.commons:TabStrip>
	
	
	

	<!-- License info end -->
	</VBox>
</sap.ui.core:FragmentDefinition>