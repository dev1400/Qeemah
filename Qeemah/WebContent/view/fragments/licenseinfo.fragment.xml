<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.unified="sap.ui.unified"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	
	
 <VBox  id="idLicenseInfoParentVBox" class="HeadingBGVBOX">
 <HBox justifyContent="Center">
 <IconTabBar
    class="iconTabBarPaddingTop IconTabBarStyling"
    expanded="true" expandable="false" idd="idLicenseInfoIconTabBar">
    <items>
      <IconTabFilter
        text="{i18n>BusinessActivityQuestions}" id="idLicenseInfo_BizActivityQ">
        
        <sap.ui.core:Fragment fragmentName="com.sagia.view.fragments.bizactivityquestions"
		type="XML" />
		
      </IconTabFilter>
      <IconTabFilter
        text="{i18n>LicenseInformation}" id="idLicenseInfo_LicenseInfo">
        
        <sap.ui.core:Fragment
		fragmentName="com.sagia.view.fragments.bizactivityquestions_licenseinfo"
		type="XML" />
		
      </IconTabFilter>
    </items>
  </IconTabBar>
  
	
	
	
	</HBox>
	</VBox>
	<HBox justifyContent="End" alignItems="Center" class="BottomVBoxForNEXtPrevious">

		<sap.ui.commons.layout:MatrixLayout
			layoutFixed="false">
			<sap.ui.commons.layout:rows>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:cells>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<sap.ui.commons.layout:content>
								<Button style="Accept" text="{i18n>Previous}" id="idLicenseInfoPreviousButton"
									press="" class="PreviousButtonStyling">
								</Button>
							</sap.ui.commons.layout:content>
						</sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<sap.ui.commons.layout:content>
								<Button style="Accept" text="{i18n>Next}" id="idLicenseInfoNextButton"
									press="" ></Button>
							</sap.ui.commons.layout:content>
						</sap.ui.commons.layout:MatrixLayoutCell>
					</sap.ui.commons.layout:cells>
				</sap.ui.commons.layout:MatrixLayoutRow>
			</sap.ui.commons.layout:rows>
		</sap.ui.commons.layout:MatrixLayout>



	
	</HBox>
</sap.ui.core:FragmentDefinition>