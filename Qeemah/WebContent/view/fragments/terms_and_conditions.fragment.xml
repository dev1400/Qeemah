	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:sap.ui.unified="sap.ui.unified" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">

	
		<VBox class="HeadingBGVBOX">
		
		<sap.ui.commons:TabStrip class="IconTabBarStyling"
				>
				<sap.ui.commons:tabs>
					<sap.ui.commons:Tab  text="{i18n>TermsnCond}">
						<sap.ui.commons:content>
						<ScrollContainer height="39.88rem" width="69rem"
		vertical="true">
<html:br/>
						<VBox justifyContent="Center" alignItems="Center">

							<sap.ui.commons.layout:MatrixLayout
								layoutFixed="false">
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
													<sap.ui.commons:TextView text="{i18n>Name}"></sap.ui.commons:TextView>
<!-- 													<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
 -->												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
													<Input type="Text" width="25em" />
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>

									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
													<sap.ui.commons:TextView text="{i18n>Role}"></sap.ui.commons:TextView>
<!-- 													<sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
 -->												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
													<ComboBox width="25em">
														<sap.ui.core:ListItem Key="1" text="{i18n>Agent}" />
															<sap.ui.core:ListItem key="2" text="{i18n>Employee}" />
															<sap.ui.core:ListItem Key="3" text="{i18n>Investor}" />
															<sap.ui.core:ListItem key="4" text="{i18n>GM}" />
													</ComboBox>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>

									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
													<sap.ui.commons:TextView text="{i18n>Date}"></sap.ui.commons:TextView>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
													<Input id="idTNCDate" type="Text" width="25em" enabled="false" />

												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
									
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell colSpan="2" hAlign="Center">
												<sap.ui.commons.layout:content>
													<Button text="{i18n>ReadTermsAndConditions}" press="handleReadTermsandConditionsPress" />
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
									
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell colSpan="2" hAlign="Center">
												<sap.ui.commons.layout:content>
													<CheckBox text="{i18n>AgreeToAllTermsandConditions}" id="idTermsAndConditionsCheckBox"/>
<!-- 												    <sap.ui.commons:TextView text="*" class="textcolorred"></sap.ui.commons:TextView>
 -->													
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											
											
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
							</sap.ui.commons.layout:MatrixLayout>
						</VBox>
						</ScrollContainer>
						</sap.ui.commons:content>
					</sap.ui.commons:Tab>
					
				</sap.ui.commons:tabs>
