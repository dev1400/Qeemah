										     useMultipart="false" 
										     sameFilenameAllowed="true" 
											 id="idNSHBankStatementFileUploader"  
											 mimeType="application/pdf"/> 
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<sap.ui.commons:TextView id="idNSHBankStatementAttachmentName" visible="false"></sap.ui.commons:TextView>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
								
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<Label text="{i18n>ThreeYearsBalanceSheet}"></Label>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
												<sap.ui.commons.layout:content>
												<sap.ui.unified:FileUploader
											 width="15em"
										     sendXHR="true" 
										     useMultipart="false" 
										     sameFilenameAllowed="true" 
											 id="idNSHBalanceSheetFileUploader"  
											 mimeType="application/pdf"/> 
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<sap.ui.commons:TextView id="idNSHBalanceSheetAttachmentName" visible="false"></sap.ui.commons:TextView>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
								
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<Label text="{i18n>Other}"></Label>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
												<sap.ui.commons.layout:content>
												<sap.ui.unified:FileUploader
											 width="15em"
										     sendXHR="true" 
										     useMultipart="false" 
										     sameFilenameAllowed="true" 
											 id="idNSHOtherFileUploader"  
											 mimeType="application/pdf"/> 
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<sap.ui.commons:TextView id="idNSHOtherAttachmentName" visible="false"></sap.ui.commons:TextView>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
								
