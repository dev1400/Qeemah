<sap.ui.core:FragmentDefinition xmlns="sap.m"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:sap.ui.unified="sap.ui.unified" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<sap.ui.commons.layout:MatrixLayout
								layoutFixed="false" columns="3" width="78em">
								<sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:cells>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="Begin" colSpan="3">
						<sap.ui.commons.layout:content>
							<sap.ui.commons:TextView text="{i18n>Attachments}" design="H1"></sap.ui.commons:TextView>
							<sap.ui.commons:HorizontalDivider></sap.ui.commons:HorizontalDivider>
						</sap.ui.commons.layout:content>
					</sap.ui.commons.layout:MatrixLayoutCell>
					
					
				</sap.ui.commons.layout:cells>
			</sap.ui.commons.layout:MatrixLayoutRow>
			
			
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<Label text="{i18n>PassportCopyIDCopy}"></Label>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
												<sap.ui.commons.layout:content>
										   <sap.ui.unified:FileUploader
											 width="15em"
										     sendXHR="true" 
										     useMultipart="false" 
										     sameFilenameAllowed="true" 
											 id="idNSHPassportCopyFileUploader"  
											 mimeType="application/pdf"/> 
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<sap.ui.commons:TextView id="idNSHPassPortAttachmentName" visible="false"></sap.ui.commons:TextView>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
								
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<Label text="{i18n>CommercialRegistration}"></Label>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
												<sap.ui.commons.layout:content>
												<sap.ui.unified:FileUploader
											 width="15em"
										     sendXHR="true" 
										     useMultipart="false" 
										     sameFilenameAllowed="true" 
											 id="idNSHCommercialRegistFileUploader"  
											 mimeType="application/pdf"/> 
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
												<sap.ui.commons:TextView id="idNSHCommercialAttachmentName" visible="false"></sap.ui.commons:TextView>
												</sap.ui.commons.layout:content>
											</sap.ui.commons.layout:MatrixLayoutCell>
										</sap.ui.commons.layout:cells>
									</sap.ui.commons.layout:MatrixLayoutRow>
								</sap.ui.commons.layout:rows>
								
								<sap.ui.commons.layout:rows>
									<sap.ui.commons.layout:MatrixLayoutRow>
										<sap.ui.commons.layout:cells>
											<sap.ui.commons.layout:MatrixLayoutCell>
												<sap.ui.commons.layout:content>
