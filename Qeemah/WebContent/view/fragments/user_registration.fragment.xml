<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:sap.ui.commons.layout="sap.ui.commons.layout"
	xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:html="http://www.w3.org/1999/xhtml">

	<VBox id="idVBoxRegistration" justifyContent="Center" alignItems="Center">
		<items>



			<sap.ui.commons.layout:MatrixLayout
				layoutFixed="false" >
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						colSpan="2">
						<VBox alignItems="Center">
							<core:HTML content="{i18n>Welcometo}"></core:HTML>
							<core:HTML content="{i18n>SAGIAOnlineServices}"></core:HTML>
							<core:HTML content="{i18n>HomePageMessage}"></core:HTML>
						</VBox>

					</sap.ui.commons.layout:MatrixLayoutCell>

				</sap.ui.commons.layout:MatrixLayoutRow>

				<!-- <sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell colSpan="2">
					<HBox justifyContent="Center"><Button text="{i18n>NewRegistration}" id="idNewRegistrationButton"
							press="handleNewRegistrationButtonPress" class="RegisterButtonWidth"/>
							<Button text="{i18n>Login}" id="idLoginButton" class="LoginButtonWidth" press="handleLoginButtonPress" />
							</HBox>					
					</sap.ui.commons.layout:MatrixLayoutCell>

				</sap.ui.commons.layout:MatrixLayoutRow> -->
				
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="End">
					<Button text="{i18n>NewRegistration}" id="idNewRegistrationButton"
							press="handleNewRegistrationButtonPress" width="11rem" />
							
							
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell hAlign="Begin">
					<Button text="{i18n>Login}" id="idLoginButton" width="11rem" press="handleLoginButtonPress" />
					</sap.ui.commons.layout:MatrixLayoutCell>

				</sap.ui.commons.layout:MatrixLayoutRow>
			</sap.ui.commons.layout:MatrixLayout>






			<sap.ui.commons.layout:MatrixLayout
				layoutFixed="false" columns="2" id="idRegistrationMatrixLayout"
				visible="false">
				

				<sap.ui.commons.layout:MatrixLayoutRow>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Label text="*" class="redcolortext"></Label>
						<Label text="{i18n>PleaseEnterFirstName}" class="textcolorwhite"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Input type="Text" required="true" id="idFirstNameInputText" />
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>

				<sap.ui.commons.layout:MatrixLayoutRow>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Label text="*" class="redcolortext"></Label>
						<Label text="{i18n>PleaseEnterLastName}" class="textcolorwhite"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Input type="Text" required="true" id="idLastNameInputText" />
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>





				<sap.ui.commons.layout:MatrixLayoutRow>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Label text="*" class="redcolortext"></Label>
						<Label text="{i18n>PleaseEnterPassword}" class="textcolorwhite"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Input type="Text" required="true" id="idInputPassword" />
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>

				<sap.ui.commons.layout:MatrixLayoutRow>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Label text="*" class="redcolortext"></Label>
						<Label text="{i18n>PleaseReEnterPassword}" class="textcolorwhite"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Input type="Text" required="true" id="idInputRePassword" />
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>

				<sap.ui.commons.layout:MatrixLayoutRow>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Label text="*" class="redcolortext"></Label>
						<Label text="{i18n>PleaseEnterMobileNumber}" class="textcolorwhite"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Input type="Text" required="true" id="idInputMobileNumber" />
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>

				<sap.ui.commons.layout:MatrixLayoutRow>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Label text="*" class="redcolortext"></Label>
						<Label text="{i18n>PleaseEnterEmail}" class="textcolorwhite"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>

					<sap.ui.commons.layout:MatrixLayoutCell>
						<Input type="Text" required="true" id="idInputEmail" />
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>







				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						colSpan="2" hAlign="Center">
						<VBox justifyContent="Inherit" alignItems="Stretch" width="100%">
							<Button style="Accept" text="{i18n>Register}"
								press="handleRegisterUserButtonPress" width="100%" class="buttonsBackGround">
							</Button>
						</VBox>

					</sap.ui.commons.layout:MatrixLayoutCell>

				</sap.ui.commons.layout:MatrixLayoutRow>



			</sap.ui.commons.layout:MatrixLayout>



			<VBox id="idLoginVBoxLayout" justifyContent="Center" alignItems="Center"
				width="100%">

				<sap.ui.commons.layout:MatrixLayout
					layoutFixed="false" columns="2">
					<sap.ui.commons.layout:MatrixLayoutRow>

						<sap.ui.commons.layout:MatrixLayoutCell>
							<Label text="*" class="redcolortext"></Label>
							<Label text="{i18n>PleaseEnterInvestorID}" class="textcolorwhite"></Label>
						</sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<Input id="idSignInUsernameInput" required="true" />
						</sap.ui.commons.layout:MatrixLayoutCell>
					</sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutRow>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<Label text="*" class="redcolortext"></Label>
							<Label text="{i18n>PleaseEnterPassword}" class="textcolorwhite"></Label>
						</sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<Input type="Password" id="idSignInPasswordInput" required="true" />
						</sap.ui.commons.layout:MatrixLayoutCell>
					</sap.ui.commons.layout:MatrixLayoutRow>
				</sap.ui.commons.layout:MatrixLayout>






				<sap.ui.commons.layout:MatrixLayout
					layoutFixed="false" columns="2">

					<sap.ui.commons.layout:MatrixLayoutRow>
						<sap.ui.commons.layout:MatrixLayoutCell
							hAlign="End">
							<Button style="Accept" text="{i18n>SignIn}" press="handleSignInButtonPress"></Button>
						</sap.ui.commons.layout:MatrixLayoutCell>

						<sap.ui.commons.layout:MatrixLayoutCell>
							<Button style="Reject" text="{i18n>Cancel}" press="handleCancelButtonPress"></Button>
						</sap.ui.commons.layout:MatrixLayoutCell>

					</sap.ui.commons.layout:MatrixLayoutRow>
				</sap.ui.commons.layout:MatrixLayout>
			</VBox>





		</items>
	</VBox>
</core:FragmentDefinition>