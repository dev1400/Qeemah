<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:sap.ui.commons.layout="sap.ui.commons.layout" xmlns:sap.ui.commons="sap.ui.commons"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox id="idVBoxRegistration" justifyContent="Center" alignItems="Center">
		<items>
			<sap.ui.commons.layout:MatrixLayout
				layoutFixed="false" columns="2">
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						colSpan="2">
						<VBox alignItems="Center">
									<core:HTML content="{i18n>RegistrationFormHeading}"></core:HTML>
						</VBox>
					</sap.ui.commons.layout:MatrixLayoutCell>

				</sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
							<Label text="{i18n>EnterFirstName}"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons:TextField required="true"/>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
							<Label text="{i18n>EnterLastName}"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons:TextField required="true"/>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
							<Label text="{i18n>EnterMobileNumber}"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons:TextField required="true"/>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
							<Label text="{i18n>EnterEmail}"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons:TextField required="true"/>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
							<Label text="{i18n>EnterPassword}"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons:TextField required="true"/>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
							<Label text="{i18n>ReEnterPassword}"></Label>
					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons:TextField required="true"/>
					</sap.ui.commons.layout:MatrixLayoutCell>
				</sap.ui.commons.layout:MatrixLayoutRow>
			</sap.ui.commons.layout:MatrixLayout>
			<HBox justifyContent="Center">
				<sap.ui.commons.layout:MatrixLayout
					layoutFixed="false">
					<sap.ui.commons.layout:MatrixLayoutRow>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<sap.ui.commons:Button style="Accept" text="{i18n>Register}" press="handleRegisterOrgButtonPress">
							</sap.ui.commons:Button>
						</sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons.layout:MatrixLayoutCell>
							<sap.ui.commons:Button style="Reject" text="{i18n>Cancel}" press="onPress">
							</sap.ui.commons:Button>
						</sap.ui.commons.layout:MatrixLayoutCell>
						<sap.ui.commons.layout:MatrixLayoutCell>
						</sap.ui.commons.layout:MatrixLayoutCell>
					</sap.ui.commons.layout:MatrixLayoutRow>
				</sap.ui.commons.layout:MatrixLayout>
			</HBox>
		</items>
	</VBox>
</core:FragmentDefinition>