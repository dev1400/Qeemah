<mvc:View height="100%" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
	xmlns:u="sap.ui.unified" xmlns:sap.ui.core="sap.ui.core" xmlns:form="sap.ui.layout.form"
	xmlns:sap.ui.commons.layout="sap.ui.commons.layout" xmlns="sap.m"
	xmlns:sap.ui.commons="sap.ui.commons" controllerName="com.sagia.view.Overview"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="idPage" enableScrolling="true" showHeader="false"><!-- backgroundDesign="Transparent" -->

		
		<VBox id="idMainPageContent">
			<VBox justifyContent="Center" alignItems="Center" class="MainVBoxBGIMG">
				<html:br />
				<html:br />
				<sap.ui.core:Fragment fragmentName="com.sagia.view.fragments.user_registration"
					type="XML" />

			</VBox>

			<sap.ui.commons.layout:MatrixLayout>
				<sap.ui.commons.layout:MatrixLayoutRow>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="End">
						<HBox justifyContent="End" alignItems="Center">
							<Image src="common/mime/dot.png"></Image>
							<Link text="{i18n>ServiceGuide}" class="linkColor" />
						</HBox>


					</sap.ui.commons.layout:MatrixLayoutCell>
					<sap.ui.commons.layout:MatrixLayoutCell
						hAlign="Begin">
						<HBox justifyContent="Start" alignItems="Center">
							<Image src="common/mime/dot.png"></Image>
							<Link text="{i18n>DownloadForms}" class="linkColor" />
						</HBox>
					</sap.ui.commons.layout:MatrixLayoutCell>


				</sap.ui.commons.layout:MatrixLayoutRow>

			</sap.ui.commons.layout:MatrixLayout>
		</VBox>



		<!-- Left hand buttons start -->

		<VBox id="idLicenseButtonsHBox" visible="false" justifyContent="Center"
			width="100%">
			<HBox class="MainLicenseRegBGIMG" width="100%" justifyContent="Start">
				<html:button class="BasicInfoButtonBGLogo">
					<html:img src="common/mime/innerlogo.png" style="vertical-align:middle;" />
				</html:button>
				<VBox width="100%">

					<HBox width="100%" justifyContent="End" alignItems="Center">
						<sap.ui.commons.layout:MatrixLayout
							layoutFixed="false" columns="3">
							<sap.ui.commons.layout:rows>
								<sap.ui.commons.layout:MatrixLayoutRow>
									<sap.ui.commons.layout:cells>
										<sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.commons.layout:content>
												<SearchField width="15em" />
											</sap.ui.commons.layout:content>
										</sap.ui.commons.layout:MatrixLayoutCell>
										<sap.ui.commons.layout:MatrixLayoutCell>
											<sap.ui.core:Icon src="sap-icon://log"></sap.ui.core:Icon>
										</sap.ui.commons.layout:MatrixLayoutCell>
										<sap.ui.commons.layout:MatrixLayoutCell>
											<Link text="{i18n>Logout}" class="linkColorBlue" />
										</sap.ui.commons.layout:MatrixLayoutCell>
									</sap.ui.commons.layout:cells>
								</sap.ui.commons.layout:MatrixLayoutRow>
							</sap.ui.commons.layout:rows>
						</sap.ui.commons.layout:MatrixLayout>





					</HBox>
					<html:br />
					<HBox width="100%" justifyContent="SpaceBetween">
					<HBox alignItems="Center" class="BasicInfoTextxPaddingLeft">
						<sap.ui.core:HTML content="{i18n>BasicInformationHTML}" ></sap.ui.core:HTML>
						</HBox>
						<HBox alignItems="Center">
							<Image src="common/mime/pin_logo.png" class="CompanyLogoPaddingRightIMG"></Image>
							<ComboBox width="9em" class="LanguageComboBoxPaddingRight">
								<sap.ui.core:Item />
							</ComboBox>
						</HBox>


					</HBox>
				</VBox>
			</HBox>
			<HBox>
				<VBox class="BasicInfoButtonBGVBOX">
				<VBox class="BasicInfoButtonBG">
				<Image src="common/mime/basicinfo.png" />
				<Image src="common/mime/license.png" />
				<Image src="common/mime/shareholder.png" />
				<Image src="common/mime/preview.png" />
				<Image src="common/mime/terms.png" />
				<Image src="common/mime/basicinfo.png" />
				</VBox>
				
					<!-- <html:button class="BasicInfoButtonBG" id="idBasicInfoButton" onClick="handleBasicInfoButtonClick">
						<html:img src="common/mime/basicinfo.png" />
					</html:button>
					<html:button class="BasicInfoButtonBG" id="idLicenseInfoButton">
						<html:img src="common/mime/license.png" />
					</html:button>
					<html:button class="BasicInfoButtonBG" id="idShareholderInfoButton">
						<html:img src="common/mime/shareholder.png" />
					</html:button>
					<html:button class="BasicInfoButtonBG" id="idPreviewInfoButton">
						<html:img src="common/mime/preview.png" />
					</html:button>
					<html:button class="BasicInfoButtonBG" id="idTermsInfoButton">
						<html:img src="common/mime/terms.png" />
					</html:button>
					<html:button class="BasicInfoButtonBG" id="idSubmitInfoButton">
						<html:img src="common/mime/submit.png" />
					</html:button> -->
				</VBox>


				<VBox id="idBasicInfoContent" width="100%">

					<sap.ui.core:Fragment fragmentName="com.sagia.view.fragments.basicinfo"
						type="XML" />
				</VBox>
			</HBox>
		</VBox>

		<!-- Left hand buttons end -->



	</Page>
</mvc:View>
