<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="com.sagia.view.SecondPage" xmlns:layout="sap.ui.commons.layout"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Special Track">
		<content>
			<VBox>
				<Input type="Text" placeholder="{i18n>EnterUsername}" />
				<Input type="Text" placeholder="{i18n>EnterPassword}" />
				<HBox justifyContent="Center">
					<layout:MatrixLayout id="idMatrixLayout"
						layoutFixed="false">
						<layout:MatrixLayoutRow>
							<layout:MatrixLayoutCell>
								<Button type="Accept" text="{i18n>Next}" press="nextPage">
								</Button>
							</layout:MatrixLayoutCell>
							<layout:MatrixLayoutCell>
								<Button type="Reject" text="{i18n>Back}" press="goBack">
								</Button>
							</layout:MatrixLayoutCell>
							

						</layout:MatrixLayoutRow>
					</layout:MatrixLayout>
				</HBox>
			</VBox>
		</content>
	</Page>
</core:View>